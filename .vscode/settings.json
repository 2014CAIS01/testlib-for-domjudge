{
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "cSpell.words": [
        "cubercsl",
        "Domjudge",
        "interactor",
        "interactors",
        "kattis",
        "memorylimit",
        "outputlimit",
        "pypi",
        "pyright",
        "testlib",
        "testset",
        "timelimit"
    ],
}
